#################################################################
#    General axioms
#################################################################

[ owl:intersectionOf ( sc:MensClothing
                       sc:WomensClothing
                     ) ;
  rdf:type owl:Class ;
  rdfs:subClassOf dbr:Clothing
] .


[ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                         owl:onProperty sc:hasColour ;
                         owl:hasValue dbr:Black
                       ]
                       [ rdf:type owl:Restriction ;
                         owl:onProperty sc:isSuitableToBeDressedAtEvent ;
                         owl:hasValue sc:Funneral
                       ]
                     ) ;
  rdf:type owl:Class ;
  rdfs:subClassOf dbr:Clothing
] .


[ rdf:type owl:Restriction ;
  owl:onProperty sc:isSuitableToBeDressedByGenre ;
  owl:hasValue dbr:Female ;
  rdfs:subClassOf dbr:Clothing
] .


[ rdf:type owl:Restriction ;
  owl:onProperty sc:isSuitableToBeDressedByGenre ;
  owl:hasValue dbr:Male ;
  rdfs:subClassOf dbr:Clothing
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( dbo:Colour
