@prefix : <http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix gr: <http://purl.org/goodrelations/v1#> .
@prefix sc: <http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix dbp: <http://dbpedia.org/property/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix muto: <http://purl.org/muto/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sioc: <http://rdfs.org/sioc/ns#> .
@prefix owl2xml: <http://www.w3.org/2006/12/owl2-xml#> .
@base <http://www.semanticweb.org/ontologies/2015/02/semcloth.owl> .

<http://www.semanticweb.org/ontologies/2015/02/semcloth.owl> rdf:type owl:Ontology ;
                                                              rdfs:label "A common vocabulary for clothing items."@en ;
                                                              dc:bibliographicCitation "*** H&M, http://www.hm.com/ro/ (provided support for a few of the concepts used in this ontology)"@en ,
                                                                                       "***, flaticon, http://www.flaticon.com/ (Icons for the clothes)"@en ,
                                                                                       "***, W3C, http://www.w3.org/2001/sw/"@en ;
                                                              dc:contributor "Oana Gagea"@en ;
                                                              dc:bibliographicCitation "***, ZARA, http://www.zara.com/ro/en/ (provided support for serveral concepts used in this ontology)"@en ;
                                                              dc:date "2015-02"^^xsd:gYearMonth ;
                                                              rdfs:label "Semantic Clothing Ontology"@en ;
                                                              dc:contributor "Iordan Rață"@en ;
                                                              dc:creator "Daniel Amariei"@en ;
                                                              dc:contributor "Ioana Bîrsan"@en ;
                                                              dc:bibliographicCitation "***, Dr. Sabin-Corneliu Buraga, Web Applications Development (Fall 2014), (RO) http://profs.info.uaic.ro/~busaco/teach/courses/wade/index.html, UAIC, FII (this resource provided serveral useful examples from which this ontolgy was modeled after, together with theoretical support)."@en ;
                                                              rdfs:comment "The purpose of this ontology is to create the common conceptual framwork for 'talking' about clothing or related concepts."@en ;
                                                              dc:bibliographicCitation "***, Haine Grasute, http://hainegrasute.blogspot.ro/2013/05/cum-sa-te-imbraci-avantajos-in-functie.html (provided support for modelling the 'body shapes' part of the ontology)"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://dbpedia.org/ontology/thumbnail
dbo:thumbnail rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://rdfs.org/sioc/ns#note
sioc:note rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#comment
rdfs:comment rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#domain
rdfs:domain rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://dbpedia.org/property/composition
dbp:composition rdf:type owl:ObjectProperty .


###  http://dbpedia.org/property/dressSize
dbp:dressSize rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf sc:hasSize ;
              rdfs:domain sc:Dress ;
              rdfs:range dbr:Clothing_sizes ;
              rdfs:label "Dress size"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#bodyShapeHasSuitableClothingForDressing
sc:bodyShapeHasSuitableClothingForDressing rdf:type owl:ObjectProperty ;
                                           owl:inverseOf sc:isSuitableToBeDressedByBodyShape ;
                                           rdfs:domain sc:BodyShape ;
                                           rdfs:range dbr:Clothing ;
                                           rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular body shape."@en ;
                                           rdfs:label "Body shape has suitable clothing for dressing"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#dressingCodeHasSuitableClothingForDressing
sc:dressingCodeHasSuitableClothingForDressing rdf:type owl:ObjectProperty ;
                                              owl:inverseOf sc:isSuitableForDressingCode ;
                                              rdfs:domain dbr:Dress_code ,
                                                          sc:ClothingStyle ;
                                              rdfs:range dbr:Clothing ;
                                              rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular dressing code."@en ;
                                              rdfs:label "Dressing code has suitable clothing for dressing"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#eventHasSuitableClothingForDressing
sc:eventHasSuitableClothingForDressing rdf:type owl:ObjectProperty ;
                                       owl:inverseOf sc:isSuitableToBeDressedAtEvent ;
                                       rdfs:domain dbo:Event ;
                                       rdfs:range dbr:Clothing ;
                                       rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular event."@en ;
                                       rdfs:label "Event has suitable clothing for dressing"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#genreHasSuitableClothingForDressing
sc:genreHasSuitableClothingForDressing rdf:type owl:ObjectProperty ;
                                       owl:inverseOf sc:isSuitableToBeDressedByGenre ;
                                       rdfs:domain dbr:Genre ;
                                       rdfs:range dbr:Clothing ;
                                       rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular genre."@en ;
                                       rdfs:label "Genre has suitable clothing for dressing"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#hasColour
sc:hasColour rdf:type owl:ObjectProperty ;
             rdfs:domain dbr:Clothing ;
             rdfs:range dbo:Colour .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#hasSize
sc:hasSize rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain dbr:Clothing ;
           rdfs:range dbr:Clothing_sizes ;
           rdfs:comment "This property specifies the size of a particular clothing item."@en ;
           rdfs:label "Has size"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#hasTextileComposition
sc:hasTextileComposition rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf dbp:composition ;
                         rdfs:domain dbr:Clothing ;
                         rdfs:range sc:ClothingMaterial ;
                         rdfs:comment "This property denotes the materials from which a clothing item is composed of."@en ;
                         rdfs:label "Has textile composition"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#hasTexture
sc:hasTexture rdf:type owl:ObjectProperty ;
              rdfs:domain dbr:Clothing ;
              rdfs:range sc:ClothingTexture ;
              rdfs:comment "Specifies the texture of the clothing item."@en ;
              rdfs:label "Has texture" .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isManufacteredBy
sc:isManufacteredBy rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ,
                             owl:AsymmetricProperty ;
                    rdfs:domain dbr:Clothing ;
                    rdfs:range gr:BusinessEntity ;
                    rdfs:label "Is manufactured by"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableForDressingCode
sc:isSuitableForDressingCode rdf:type owl:ObjectProperty ;
                             rdfs:domain dbr:Clothing ;
                             rdfs:range dbr:Dress_code ,
                                        sc:ClothingStyle ;
                             rdfs:comment "This property specifis the dressing code for which a particular clothing item and/or class is/are suitable for."@en ;
                             rdfs:label "Is suitable for dressing code"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableForDressingForReligion
sc:isSuitableForDressingForReligion rdf:type owl:ObjectProperty ;
                                    owl:inverseOf sc:religionHasSuitableClothingForDressing ;
                                    rdfs:domain dbr:Clothing ;
                                    rdfs:range dbr:Religion ;
                                    rdfs:comment "This property specifis the religion for which a particular clothing item and/or class is/are suitable for."@en ;
                                    rdfs:label "Is suitable for dressing for religion"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableToBeDressedAtEvent
sc:isSuitableToBeDressedAtEvent rdf:type owl:ObjectProperty ;
                                rdfs:domain dbr:Clothing ;
                                rdfs:range dbo:Event ;
                                rdfs:comment "This property specifies the Event for which a particular clothing item and/or class is/are suitable for."@en ;
                                rdfs:label "Is suitable to be dressed at event"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableToBeDressedByBodyShape
sc:isSuitableToBeDressedByBodyShape rdf:type owl:ObjectProperty ;
                                    rdfs:domain dbr:Clothing ;
                                    rdfs:range sc:BodyShape ;
                                    rdfs:comment "Stipulates for which body shape a particular clothing item and/or class is/are suitable for."@en ;
                                    rdfs:label "Is suitable to be dressed by body shape"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableToBeDressedByGenre
sc:isSuitableToBeDressedByGenre rdf:type owl:ObjectProperty ;
                                rdfs:domain dbr:Clothing ;
                                rdfs:range dbr:Genre ;
                                rdfs:comment "This property specifies the genre(s) for which a clothing item is suitable for. We need to make this general enough since some clothing items can be suitable for both sexes."@en ;
                                rdfs:label "Is suitable to be dressed by genre"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableToBeDressedInSeason
sc:isSuitableToBeDressedInSeason rdf:type owl:ObjectProperty ;
                                 owl:inverseOf sc:seasonHasSuitableClothingForDressing ;
                                 rdfs:domain dbr:Clothing ;
                                 rdfs:range dbr:Season ;
                                 rdfs:comment "This property specifies the season for which a particular clothing item and/or class is/are suitable for."@en ;
                                 rdfs:label "Is suitable to be dressed in season"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#isSuitableToBeDressedOnWeather
sc:isSuitableToBeDressedOnWeather rdf:type owl:ObjectProperty ;
                                  owl:inverseOf sc:weatherHasSuitableClothingForDressing ;
                                  rdfs:domain dbr:Clothing ;
                                  rdfs:range dbr:Weather ;
                                  rdfs:comment "This property specifis the weather conditions for which a particular clothing item and/or class is/are suitable for."@en ;
                                  rdfs:label "Is suitable to be dressed in weather conditions"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#religionHasSuitableClothingForDressing
sc:religionHasSuitableClothingForDressing rdf:type owl:ObjectProperty ;
                                          rdfs:domain dbr:Religion ;
                                          rdfs:range dbr:Clothing ;
                                          rdfs:comment "This property specifis the clothing that is suitable for a particular religion."@en ;
                                          rdfs:label "Religion has suitable clothing for dressing"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#seasonHasSuitableClothingForDressing
sc:seasonHasSuitableClothingForDressing rdf:type owl:ObjectProperty ;
                                        rdfs:domain dbr:Season ;
                                        rdfs:range dbr:Clothing ;
                                        rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular season."@en ;
                                        rdfs:label "Season has suitable clothing for dressing"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#weatherHasSuitableClothingForDressing
sc:weatherHasSuitableClothingForDressing rdf:type owl:ObjectProperty ;
                                         rdfs:domain dbr:Weather ;
                                         rdfs:range dbr:Clothing ;
                                         rdfs:comment "This property specifies the clothing item(s) that is/are suitable for a particular weather condition."@en ;
                                         rdfs:label "Weather has suitable clothing for dressing"@en .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Colour
dbo:Colour rdf:type owl:Class .


###  http://dbpedia.org/ontology/Event
dbo:Event rdf:type owl:Class ;
          owl:disjointUnionOf ( sc:BusinessMeeting
                                sc:Club
                                sc:Date
                                sc:Funneral
                                sc:PyjamaParty
                              ) .


###  http://dbpedia.org/resource/Clothing
dbr:Clothing rdf:type owl:Class ;
             owl:disjointUnionOf ( sc:Accessory
                                   sc:Blazer
                                   sc:Coat
                                   sc:Dress
                                   sc:Gloves
                                   sc:Jeans
                                   sc:Pyjama
                                   sc:Raincoat
                                   sc:Shirt
                                   sc:Shoes
                                   sc:Skirt
                                   sc:Suit
                                   sc:TShirt
                                   sc:Top
                                   sc:Trousers
                                   sc:Underwear
                                 ) ;
             rdfs:comment "Represents a clothing item (e.g. Skirt, Jeans, Shoes, etc.) or a collection of clothing items (e.g. a suit -- which is formed from multiple items.). We need to abstract the posibility that a clothing item can be formed from one or multiple clothing items."@en ;
             rdfs:label "Clothing"@en .


###  http://dbpedia.org/resource/Clothing_sizes
dbr:Clothing_sizes rdf:type owl:Class .


###  http://dbpedia.org/resource/Dress_code
dbr:Dress_code rdf:type owl:Class .


###  http://dbpedia.org/resource/Female
dbr:Female rdf:type owl:Class ;
           owl:disjointWith dbr:Male .


###  http://dbpedia.org/resource/Genre
dbr:Genre rdf:type owl:Class ;
          owl:disjointUnionOf ( dbr:Female
                                dbr:Male
                              ) .


###  http://dbpedia.org/resource/Male
dbr:Male rdf:type owl:Class .


###  http://dbpedia.org/resource/Raw_material
dbr:Raw_material rdf:type owl:Class .


###  http://dbpedia.org/resource/Religion
dbr:Religion rdf:type owl:Class .


###  http://dbpedia.org/resource/Season
dbr:Season rdf:type owl:Class .


###  http://dbpedia.org/resource/Shape
dbr:Shape rdf:type owl:Class .


###  http://dbpedia.org/resource/Weather
dbr:Weather rdf:type owl:Class ;
            rdfs:subClassOf owl:Thing .


###  http://purl.org/goodrelations/v1#BusinessEntity
gr:BusinessEntity rdf:type owl:Class .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Accessory
sc:Accessory rdf:type owl:Class ;
             rdfs:subClassOf dbr:Clothing ;
             owl:disjointUnionOf ( sc:Bag
                                   sc:Belt
                                   sc:Hat
                                   sc:Jewellery
                                   sc:Scarf
                                   sc:Sunglasses
                                   sc:Tie
                                   sc:Umbrella
                                 ) ;
             dbo:thumbnail "https://raw.githubusercontent.com/danielamariei/clothing-ontology/master/thumbnails/accesory.png"^^xsd:anyURI ;
             rdfs:label "Accessory"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#AnkleBoots
sc:AnkleBoots rdf:type owl:Class ;
              rdfs:subClassOf sc:Shoes .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Bag
sc:Bag rdf:type owl:Class ;
       rdfs:subClassOf sc:Accessory ;
       owl:disjointUnionOf ( sc:Handbag
                             sc:ShoulderBag
                           ) .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Belt
sc:Belt rdf:type owl:Class ;
        rdfs:subClassOf sc:Accessory .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Blazer
sc:Blazer rdf:type owl:Class ;
          rdfs:subClassOf dbr:Clothing ;
          dbo:thumbnail "https://raw.githubusercontent.com/danielamariei/clothing-ontology/master/thumbnails/blazer-1.png"^^xsd:anyURI ;
          rdfs:label "Blazer"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#BodyShape
sc:BodyShape rdf:type owl:Class ;
             rdfs:subClassOf dbr:Shape .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Bodysuit
sc:Bodysuit rdf:type owl:Class ;
            rdfs:subClassOf sc:Lingerie ;
            dbo:thumbnail "https://raw.githubusercontent.com/danielamariei/clothing-ontology/master/thumbnails/bodysuit.png"^^xsd:anyURI ;
            rdfs:label "Bodysuit"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Boots
sc:Boots rdf:type owl:Class ;
         rdfs:subClassOf sc:Shoes .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Bra
sc:Bra rdf:type owl:Class ;
       rdfs:subClassOf sc:Lingerie ;
       owl:disjointUnionOf ( sc:PushUpBra
                             sc:ShapeLingerie
                             sc:SportBra
                           ) .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Bracelet
sc:Bracelet rdf:type owl:Class ;
            rdfs:subClassOf sc:Jewellery ;
            dbo:thumbnail "https://raw.githubusercontent.com/danielamariei/clothing-ontology/master/thumbnails/bracelet.png"^^xsd:anyURI ;
            rdfs:label "Bracelet"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Briefs
sc:Briefs rdf:type owl:Class ;
          rdfs:subClassOf sc:Lingerie ;
          dbo:thumbnail "https://raw.githubusercontent.com/danielamariei/clothing-ontology/master/thumbnails/briefs.png"^^xsd:anyURI ;
          rdfs:label "Briefs"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#BusinessMeeting
sc:BusinessMeeting rdf:type owl:Class ;
                   rdfs:subClassOf dbo:Event .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingCharacteristic
sc:ClothingCharacteristic rdf:type owl:Class ;
                          owl:disjointUnionOf ( dbo:Colour
                                                dbr:Clothing_sizes
                                                sc:BodyShape
                                                sc:ClothingMaterial
                                                sc:ClothingStyle
                                                sc:ClothingTexture
                                              ) ;
                          rdfs:label "Clothing characteristic"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingMaterial
sc:ClothingMaterial rdf:type owl:Class ;
                    rdfs:subClassOf dbr:Raw_material ;
                    owl:disjointUnionOf ( sc:NaturalClothingMaterial
                                          sc:SyntheticClothingMaterial
                                        ) .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingStyle
sc:ClothingStyle rdf:type owl:Class .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#ClothingTexture
sc:ClothingTexture rdf:type owl:Class .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Club
sc:Club rdf:type owl:Class ;
        rdfs:subClassOf dbo:Event .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Coat
sc:Coat rdf:type owl:Class ;
        rdfs:subClassOf dbr:Clothing .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Date
sc:Date rdf:type owl:Class ;
        rdfs:subClassOf dbo:Event .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#DinnerParty
sc:DinnerParty rdf:type owl:Class ;
               rdfs:subClassOf dbo:Event .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Dress
sc:Dress rdf:type owl:Class ;
         rdfs:subClassOf dbr:Clothing .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Earrings
sc:Earrings rdf:type owl:Class ;
            rdfs:subClassOf sc:Jewellery .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Flats
sc:Flats rdf:type owl:Class ;
         rdfs:subClassOf sc:Shoes .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Funneral
sc:Funneral rdf:type owl:Class ;
            rdfs:subClassOf dbo:Event .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#FunneralClothing
sc:FunneralClothing rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                 owl:onProperty sc:hasColour ;
                                                                 owl:hasValue dbr:Black
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty sc:isSuitableToBeDressedAtEvent ;
                                                                 owl:hasValue sc:Funneral
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf dbr:Clothing ;
                    rdfs:comment "Clothing that is suitable for a funneral."@en ;
                    rdfs:label "Funneral clothing"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Gloves
sc:Gloves rdf:type owl:Class ;
          rdfs:subClassOf dbr:Clothing .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Handbag
sc:Handbag rdf:type owl:Class ;
           rdfs:subClassOf sc:Bag .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Hat
sc:Hat rdf:type owl:Class ;
       rdfs:subClassOf sc:Accessory ;
       dbo:thumbnail "https://raw.githubusercontent.com/danielamariei/clothing-ontology/master/thumbnails/hat.png"^^xsd:anyURI ;
       rdfs:label "Hat"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#HighHeels
sc:HighHeels rdf:type owl:Class ;
             rdfs:subClassOf sc:Shoes .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Interview
sc:Interview rdf:type owl:Class ;
             rdfs:subClassOf sc:BusinessMeeting ;
             rdfs:label "Interview"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Jeans
sc:Jeans rdf:type owl:Class ;
         rdfs:subClassOf dbr:Clothing ;
         dbo:thumbnail "https://raw.githubusercontent.com/danielamariei/clothing-ontology/master/thumbnails/jeans.png"^^xsd:anyURI ;
         rdfs:label "Jeans"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Jewellery
sc:Jewellery rdf:type owl:Class ;
             rdfs:subClassOf sc:Accessory ;
             owl:disjointUnionOf ( sc:Bracelet
                                   sc:Earrings
                                   sc:Necklace
                                   sc:Ring
                                 ) .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Knickers
sc:Knickers rdf:type owl:Class ;
            rdfs:subClassOf sc:Lingerie ;
            dbo:thumbnail "https://raw.githubusercontent.com/danielamariei/clothing-ontology/master/thumbnails/knickers.png"^^xsd:anyURI ;
            rdfs:label "Knickers"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Lingerie
sc:Lingerie rdf:type owl:Class ;
            rdfs:subClassOf sc:Underwear ;
            owl:disjointUnionOf ( sc:Bodysuit
                                  sc:Bra
                                  sc:Briefs
                                  sc:Knickers
                                  sc:Nylons
                                ) ;
            dbo:thumbnail "https://raw.githubusercontent.com/danielamariei/clothing-ontology/master/thumbnails/lingerie.png"^^xsd:anyURI ;
            rdfs:label "Lingerie"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#MensClothing
sc:MensClothing rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty sc:isSuitableToBeDressedByGenre ;
                                      owl:hasValue dbr:Male
                                    ] ;
                rdfs:label "Men's clothing"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Moccasins
sc:Moccasins rdf:type owl:Class ;
             rdfs:subClassOf sc:Shoes .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#NaturalClothingMaterial
sc:NaturalClothingMaterial rdf:type owl:Class ;
                           rdfs:subClassOf sc:ClothingMaterial ;
                           owl:disjointWith sc:SyntheticClothingMaterial ;
                           rdfs:label "Natural clothing material"@en .


###  http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#Necklace
sc:Necklace rdf:type owl:Class ;
            rdfs:subClassOf sc:Jewellery .


